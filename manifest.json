{
	"name": "YouTube Swapper",
	"description": "My Extension",
	"version": "0.1",
	"version_name": "Alpha 1.0",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"]
	},
	"page_action": {
		"default_icon": "youtube_swapper_icon_128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/*"],
			"run_at": "document_idle",
			"js": ["content_script.js"]
		}
	],
	"permissions": [
		"declarativeContent"
	]
}